{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "clean": "if [ -d dist ]; then  rm -rf dist; fi",
    "build": "npm run clean && ./node_modules/typescript/bin/tsc && npm run copy-templates",
    "development": "nodemon -w src --ext ts --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "local:up": "docker-compose up -d",
    "local:up:build": "docker-compose up -d --build",
    "local:down": "docker-compose down",
    "local:destroy": "docker-compose down -v",
    "local:redeploy": "npm run local:destroy && npm run local:up:build",
    "local:logs": "docker logs",
    "local:api:recreate": "docker-compose up -d --force-recreate api",
    "typeorm": "ts-node --files -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ormconfig.ts",
    "typeorm:migration:create": "npm run typeorm migration:create -- -n",
    "typeorm:migration:run": "ENVIRONMENT=local TYPEORM_CLI=true ts-node ./node_modules/typeorm/cli.js migration:run",
    "typeorm:migration:runOne": "ENVIRONMENT=local TYPEORM_CLI=true ts-node ./node_modules/typeorm/cli.js migration:run",
    "typeorm:migration:revertOne": "ENVIRONMENT=local TYPEORM_CLI=true ts-node ./node_modules/typeorm/cli.js migration:revert",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.39.0",
    "@aws-sdk/s3-request-presigner": "^3.39.0",
    "apollo-server-express": "^2.12.0",
    "apollo-server-testing": "^2.23.0",
    "aws-sdk": "^2.1018.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.27.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "fuse.js": "^6.4.6",
    "graphql": "^14.7.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^12.0.0",
    "http": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.2",
    "jwt-simple": "^0.5.6",
    "lazy": "^1.0.11",
    "lodash": "^4.17.21",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@babel/register": "^7.13.16",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/graphql": "^14.5.0",
    "@types/graphql-upload": "^8.0.7",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.168",
    "@types/node": "^13.11.1",
    "@types/pg": "^8.6.1",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "nodemon": "^2.0.2",
    "prettier-eslint": "^13.0.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  }
}
